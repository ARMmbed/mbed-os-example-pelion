            "storage_tdb_internal.internal_base_address": "(1024*1024)",
            "storage_tdb_internal.internal_size"        : "(96*1024)",
            "storage.storage_type"                      : "TDB_INTERNAL"
        },
        "NUCLEO_F429ZI": {
            "target.macros_add"                         : ["MBEDTLS_USER_CONFIG_FILE=\"mbedTLSConfig_mbedOS.h\""],
